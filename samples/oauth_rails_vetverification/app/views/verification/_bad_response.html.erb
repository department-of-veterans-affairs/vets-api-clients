<div class="card bg-secondary col-md-7">
  <h5 class="card-header"><%= bad_response.message %></h5>
  <div class="card-body">
    <dl>
        <% bad_response.api_errors.each do |error| %>
        <dt>Title</dt>
        <dd><%= error['title'] %></dd>
        <dt>Detail</dt>
        <dd><%= error['detail'] %></dd>
        <% end %>
    </dl>
  </div>
</div>