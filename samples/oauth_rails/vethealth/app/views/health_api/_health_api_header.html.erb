<div class="row">
    <h1 class='col-12'>Health API Exploration</h1>
    <div class='col-12'>
        <%= link_to 'Health API Documentation &nearr;'.html_safe, 'https://developer.va.gov/explore/health/docs/argonaut', class: 'btn btn-primary', target: '_blank' %>
        <%= link_to 'Logout', logout_path, class: "btn btn-outline-warning" %>
        Authentication expires <%= distance_of_time_in_words(Time.zone.now, @authentication.expires_at) %> from <%= Time.zone.now.strftime('%l:%M %p %Z') %>.
        <small><%= link_to 'session details', authentication_path(@authentication.id) %></small>
    </div>
</div>
<br>
<% unless action_name == 'index'%>
<%= link_to '&larr; Try other Health APIs'.html_safe, health_api_path, class: 'btn btn-sm btn-outline-info' %>
<% end %>
<hr>