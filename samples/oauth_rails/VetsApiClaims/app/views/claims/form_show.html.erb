<div class="site-l-wrapper">
  <h3 class='h3 text-center'>Submitted <%= @form_number %> for <%= @veteran.present? ? "#{@veteran.name} requested by #{@name}" : @name %></h3>
  <%= render partial: "menu" %>
  <ul>
    <% @form["attributes"].each do |key, value| %>
      <li><b><%= key.capitalize %>: </b><%= value %></li>
    <% end %>
  </ul>
  <h3 class='h3'>Supporting Documents</h3>
  <%= form_tag poa_upload_path(@form['id']), multipart: true do %>
      <div class="col-4">
        <label>Add Supporting Document</label>
        <div class='form-group'>
          <%= file_field_tag :attachment, class: 'form-input' %>
        </div>
      </div>
      <div class="col-8">
        <%= submit_tag 'Upload', class: 'btn btn-primary'%>
      </div>
  <% end %>
</div>
<div class='col-md-6 offset-md-3'>
  <% if Figaro.env.debug_access_token? %>
    <hr />
    <h3>My session token</h3>

    <div class='well' id='access_token'><%= @session.access_token %></div>
  <% end %>
</div>